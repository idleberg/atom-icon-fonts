{
  "name": "icon-fonts",
  "version": "3.5.2",
  "description": "Atom snippets for a variety of icon fonts",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "android icons",
    "font awesome",
    "glyphicon",
    "semantic ui",
    "webfont"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-icon-fonts"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  },
  "scripts": {
    "build": "cd tools && yarn",
    "lint": "eslint ./snippets/*.json --ignore-path .gitignore",
    "test": "npm run lint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.json": "eslint --cache --fix"
  }
}
